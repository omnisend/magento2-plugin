<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="default">
        <job name="update_quotes" instance="Omnisend\Omnisend\Cron\UpdateQuotes" method="execute">
            <config_path>update_quotes/cron/expr</config_path>
        </job>
        <job name="update_orders" instance="Omnisend\Omnisend\Cron\UpdateOrders" method="execute">
            <config_path>update_orders/cron/expr</config_path>
        </job>
        <job name="update_customers" instance="Omnisend\Omnisend\Cron\UpdateCustomers" method="execute">
            <config_path>update_customers/cron/expr</config_path>
        </job>
        <job name="update_products" instance="Omnisend\Omnisend\Cron\UpdateProducts" method="execute">
            <config_path>update_products/cron/expr</config_path>
        </job>
        <job name="update_categories" instance="Omnisend\Omnisend\Cron\UpdateCategories" method="execute">
            <config_path>update_categories/cron/expr</config_path>
        </job>
        <job name="update_guest_subscribers" instance="Omnisend\Omnisend\Cron\UpdateGuestSubscribers" method="execute">
            <config_path>update_guest_subscribers/cron/expr</config_path>
        </job>
    </group>
</config>
